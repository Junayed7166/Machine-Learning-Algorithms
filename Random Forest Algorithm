import pandas as pd
from matplotlib import pyplot as plt
import numpy as np

#STEP 1: DATA READING AND UNDERSTANDING

from google.colab import files
uploaded = files.upload()

#View the uploaded file

import io
df = pd.read_csv(io.BytesIO(uploaded['Test Data.csv']))
df

# x = features, y= predict 

x = df.iloc[:,2:8].values
y = df.iloc[:,9:12].values

# Import train_test_split function
from sklearn.model_selection import train_test_split

# Split dataset into training set and test set
x_train, x_test, y_train, y_test = train_test_split(x, y, test_size=0.1) # 90% training and 10% test

from sklearn.ensemble import RandomForestRegressor
regression = RandomForestRegressor(n_estimators = 10, random_state = 30)
regression.fit(x_train, y_train)

y_pred = regression.predict(x_test)
y_pred

from sklearn.metrics import r2_score
r2_score(y_test, y_pred)
